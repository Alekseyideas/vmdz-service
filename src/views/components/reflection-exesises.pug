.modal.modal-exesises.fade(id="add-reflection-exesises" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="add-reflection-exesises" aria-hidden="true")
  .modal-dialog
    .modal-content
      .modal-header
        h5.modal-title Додати вправу - Заключна частина
        button.btn-close(type='button' data-bs-dismiss='modal' aria-label='Close')
      .modal-body
        .exesises
            
      .modal-footer
        button.btn.btn-secondary(type='button' data-bs-dismiss='modal') Закрити
        div.btn.btn-warning.btn-submit-ex(type='button' data-bs-dismiss='modal') Додати
